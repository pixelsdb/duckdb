# name: benchmark/tpch/parquet/parquet_q${QUERY_NUMBER_PADDED}.benchmark
# description: Execute Q${QUERY_NUMBER_PADDED} over lineitem stored in a parquet file
# group: [parquet]

name Q${QUERY_NUMBER_PADDED} (parquet)
group parquet
subgroup tpch

require parquet
require tpch

load
CREATE VIEW lineitem AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/lineitem/*');
CREATE VIEW nation AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/nation/*');
CREATE VIEW region AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/region/*');
CREATE VIEW supplier AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/supplier/*');
CREATE VIEW customer AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/customer/*');
CREATE VIEW part AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/part/*');
CREATE VIEW partsupp AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/partsupp/*');
CREATE VIEW orders AS SELECT * FROM parquet_scan('/data/9a3-03/tpch-1000/orders/*');


run extension/tpch/dbgen/queries/q${QUERY_NUMBER_PADDED}.sql
result extension/tpch/dbgen/answers/sf${SF_NUMBER}/q${QUERY_NUMBER_PADDED}.csv
