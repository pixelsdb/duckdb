# name: benchmark/tpch/parquet/pixels_q${QUERY_NUMBER_PADDED}.benchmark
# description: Execute Q${QUERY_NUMBER_PADDED} over lineitem stored in a pixels file
# group: [pixels]

name Q${QUERY_NUMBER_PADDED} (Pixels)
group pixels
subgroup tpch

require pixels
require tpch

load
CREATE VIEW orders AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/orders/v-0-ordered/*"]);
CREATE VIEW customer AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/customer/v-0-ordered/*"]);
CREATE VIEW lineitem AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/lineitem/v-0-ordered/*"]);
CREATE VIEW supplier AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/supplier/v-0-ordered/*"]);
CREATE VIEW partsupp AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/partsupp/v-0-ordered/*"]);
CREATE VIEW region AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/region/v-0-ordered/*"]);
CREATE VIEW part AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/part/v-0-ordered/*"]);
CREATE VIEW nation AS SELECT * FROM pixels_scan(["/data/9a3-04/tpch-900-e1/pixels/nation/v-0-ordered/*"]);

run extension/tpch/dbgen/queries/q${QUERY_NUMBER_PADDED}.sql
